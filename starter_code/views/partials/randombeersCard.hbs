<div class="randomCard">
    <div class="img-frame">
        <img class="random-card-img-top" src="{{image_url}}" alt="Card image cap">
    </div>
    <div class="card-body">
        <h3 class="card-title">{{name}}</h3>
        <h5 class="card-title">{{tagline}}</h5>
        <p class="card-text">{{description}}</p>
        <p> <strong> Food Pairing </strong> </p>
        <ul class="card-text"> {{#each food_pairing}}
            <li> {{this}}</li>
            {{/each}}
        </ul>
        <p class="card-text"> <strong> Brewers Tips: </strong> {{ brewers_tips}}</p>

    </div>
</div>