<div class="beerCard">
    <div class="img-frame">
        <img class="card-img-top" src="{{image_url}}" alt="Card image cap">
    </div>
    <div class="card-body">
        <h3 class="card-title">{{name}}</h3>
        <h4 class="card-title">{{tagline}}</h4>
        <p class="card-text">{{description}}</p>
    </div>
</div>